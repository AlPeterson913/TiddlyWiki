#!/usr/bin/env bash

# Build version of TiddlyWiki with external javascript
# Usage:
#  bldxjs [release]

DEFAULT_RELEASE=2.6.3.A1
RELEASE=${1:-$DEFAULT_RELEASE}
DEST=$PWD
RECIPE=$PWD/tiddlywiki_externaljs.html.recipe
ruby -Ku -C ../tools/cooker cook.rb $RECIPE -d$DEST -q -o tiddlywiki_externaljs.$RELEASE.html$2 $3 $4 $5
